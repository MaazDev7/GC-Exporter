{
  "manifest_version": 3,
  "name": "GC Exporter",
  "version": "1.0",
  "description": "GC Exporter",
  "permissions": ["debugger", "activeTab", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
